{
  "name": "@a-cube-io/cli",
  "version": "1.0.0",
  "description": "CLI tool for A-Cube e-receipt SDK development",
  "keywords": [
    "acube",
    "cli",
    "e-receipt",
    "developer-tools",
    "scaffolding",
    "webhook",
